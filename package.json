{
  "name": "notus-nextjs",
  "homepage": "https://demos.creative-tim.com/notus-nextjs",
  "version": "1.1.0",
  "description": "Notus NextJS - A free Tailwind CSS, NextJS and React Ui Kit and Admin by Creative Tim.",
  "repository": "https://github.com/creativetimofficial/notus-nextjs",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free",
    "@popperjs/core",
    "@tailwindcss/forms",
    "@testing-library/jest-dom",
    "@testing-library/react",
    "@testing-library/user-event",
    "bootstrap",
    "chart.js",
    "components",
    "firebase",
    "gulp",
    "gulp-append-prepend",
    "layouts",
    "leaflet",
    "leaflet-defaulticon-compatibility",
    "leaflet-geosearch",
    "next",
    "next-auth",
    "prop-types",
    "react",
    "react-bootstrap",
    "react-dom",
    "react-gapi",
    "react-leaflet",
    "react-router-dom",
    "react-scripts",
    "sass",
    "styles",
    "tailwindcss"
  },
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run dev",
    "build:tailwind": "tailwind build styles/index.css -o styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "autoprefixer": "10.2.5",
    "postcss": "8.2.8"
  }
}
